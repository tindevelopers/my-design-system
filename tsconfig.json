{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist",
    "rootDir": ".",
    "baseUrl": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*",
    "components/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "examples",
    "src/tailadmin",
    "src/catalyst-ui-kit/demo",
    "tailadmin-next-typescript-pro-2.0-main"
  ]
}